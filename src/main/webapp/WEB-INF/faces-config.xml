    <managed-bean>
        <managed-bean-name>profilesBean</managed-bean-name>
        <managed-bean-class>org.sai.pumpkin.web.ProfilesBean</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
        <managed-property>
